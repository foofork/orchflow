{
  "name": "@orchflow/gui",
  "version": "0.1.0",
  "private": true,
  "license": "MIT OR Apache-2.0",
  "scripts": {
    "dev": "PORT=$(node scripts/port-manager.js dev) && vite dev --port $PORT",
    "dev:test": "PORT=$(node scripts/port-manager.js test) && vite dev --port $PORT",
    "dev:e2e": "PORT=$(node scripts/port-manager.js e2e) && vite dev --port $PORT",
    "dev:visual": "PORT=$(node scripts/port-manager.js visual) && vite dev --port $PORT",
    "build": "vite build",
    "build:analyze": "npm run build && node scripts/analyze-bundle.js",
    "verify:optimizations": "node scripts/verify-optimizations.js",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "tauri": "tauri",
    "tauri:dev": "SQLX_OFFLINE=true tauri dev",
    "tauri:build": "SQLX_OFFLINE=true tauri build",
    "tauri:build:debug": "SQLX_OFFLINE=true tauri build --debug",
    "release:prepare": "npm run check && npm run build && npm run test",
    "release:build": "npm run release:prepare && npm run tauri:build",
    "release:bundle": "npm run release:build && npm run release:sign",
    "release:sign": "node scripts/sign-bundles.js",
    "updater:keygen": "bash scripts/generate-updater-keys.sh",
    "test": "npm run test:unit",
    "test:unit": "vitest run",
    "test:unit:watch": "vitest",
    "test:unit:ui": "vitest --ui",
    "test:integration": "vitest run --config vitest.config.integration.ts",
    "test:integration:watch": "vitest --config vitest.config.integration.ts",
    "test:integration:ui": "vitest --config vitest.config.integration.ts --ui",
    "test:e2e": "vitest run --config vitest.config.e2e.ts",
    "test:e2e:watch": "vitest --config vitest.config.e2e.ts",
    "test:e2e:ui": "vitest --config vitest.config.e2e.ts --ui",
    "test:e2e:smoke": "vitest run --config vitest.config.e2e.ts tests/e2e/smoke",
    "test:e2e:regression": "vitest run --config vitest.config.e2e.ts tests/e2e/regression",
    "test:e2e:critical": "vitest run --config vitest.config.e2e.ts tests/e2e/critical",
    "test:e2e:parallel": "vitest run --config vitest.config.e2e.ts --reporter=verbose",
    "test:e2e:debug": "E2E_SLOWMO=100 E2E_VIDEO=true E2E_TRACE=true vitest run --config vitest.config.e2e.ts",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e",
    "test:ci": "npm run build && npm run test:quality",
    "test:pipeline": "npm run test:audit && npm run test:migrate && npm run test:quality",
    "test:coverage": "vitest run --coverage",
    "test:performance": "vitest run --config vitest.config.performance.ts",
    "test:visual": "npm run test:visual:cleanup && playwright test",
    "test:visual:cleanup": "node scripts/cleanup-test-environment.js",
    "test:visual:update": "npm run test:visual:cleanup && playwright test --update-snapshots",
    "test:visual:percy": "npm run test:visual:cleanup && percy exec -- playwright test",
    "test:visual:ui": "npm run test:visual:cleanup && playwright test --ui",
    "test:visual:report": "playwright show-report",
    "test:visual:debug": "npm run test:visual:cleanup && playwright test --debug",
    "lighthouse": "lhci autorun",
    "lighthouse:assert": "lhci assert",
    "lighthouse:view": "lhci open",
    "bench": "vitest bench",
    "bench:watch": "vitest bench --watch",
    "bench:report": "vitest bench --reporter=json --outputFile=test-results/benchmark-results.json",
    "test:mutation": "stryker run",
    "test:mutation:incremental": "stryker run --incremental",
    "test:mutation:ci": "stryker run --concurrency 2 --timeoutMS 30000",
    "test:audit": "node scripts/test-audit.js",
    "test:migrate": "node scripts/migrate-tests.js",
    "test:coverage:integration": "vitest run --config vitest.config.integration.ts --coverage",
    "test:e2e:ci": "vitest run --config vitest.config.e2e.ts --reporter=json --outputFile=test-results/e2e-results.json",
    "test:quality": "npm run test:unit && npm run test:integration && npm run test:e2e:ci && npm run test:mutation:ci",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:test": "eslint **/*.test.ts **/*.spec.ts --config .eslintrc-test.js",
    "lint:test:fix": "eslint **/*.test.ts **/*.spec.ts --config .eslintrc-test.js --fix",
    "quality:check": "bash scripts/quality-check.sh",
    "precommit:install": "bash scripts/pre-commit-install.sh",
    "test:validate-patterns": "node scripts/validate-test-patterns.js"
  },
  "devDependencies": {
    "@lhci/cli": "^0.15.1",
    "@percy/playwright": "^1.0.8",
    "@playwright/test": "^1.54.1",
    "@stryker-mutator/core": "^9.0.1",
    "@stryker-mutator/html-reporter": "^3.1.0",
    "@stryker-mutator/typescript-checker": "^9.0.1",
    "@stryker-mutator/vitest-runner": "^9.0.1",
    "@sveltejs/adapter-static": "^3.0.8",
    "@sveltejs/kit": "^2.22.5",
    "@sveltejs/vite-plugin-svelte": "^5.1.1",
    "@tauri-apps/cli": "^2.1.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/svelte": "^5.2.8",
    "@testing-library/user-event": "^14.6.1",
    "@types/uuid": "^10.0.0",
    "@types/ws": "^8.5.9",
    "@typescript-eslint/eslint-plugin": "^8.37.0",
    "@typescript-eslint/parser": "^8.37.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "eslint": "^9.31.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.5.1",
    "eslint-plugin-svelte": "^3.10.1",
    "globals": "^16.3.0",
    "happy-dom": "^18.0.1",
    "jsdom": "^26.1.0",
    "svelte": "^5.35.7",
    "svelte-check": "^3.6.0",
    "terser": "^5.43.1",
    "tslib": "^2.4.1",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.37.0",
    "uuid": "^11.0.6",
    "vite": "^6.3.5",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@codemirror/autocomplete": "^6.18.6",
    "@codemirror/commands": "^6.7.1",
    "@codemirror/lang-javascript": "^6.2.2",
    "@codemirror/lang-json": "^6.0.1",
    "@codemirror/lang-python": "^6.1.6",
    "@codemirror/lang-rust": "^6.0.1",
    "@codemirror/lang-yaml": "^6.1.2",
    "@codemirror/language": "^6.11.2",
    "@codemirror/lint": "^6.8.5",
    "@codemirror/search": "^6.5.9",
    "@codemirror/state": "^6.4.1",
    "@codemirror/theme-one-dark": "^6.1.2",
    "@codemirror/view": "^6.35.1",
    "@tauri-apps/api": "^2.1.0",
    "@tauri-apps/plugin-dialog": "^2.3.0",
    "@tauri-apps/plugin-fs": "^2.1.0",
    "@tauri-apps/plugin-os": "^2.1.0",
    "@tauri-apps/plugin-process": "^2.1.0",
    "@tauri-apps/plugin-shell": "^2.1.0",
    "@tauri-apps/plugin-updater": "^2.1.0",
    "@xterm/addon-fit": "^0.10.0",
    "@xterm/addon-search": "^0.15.0",
    "@xterm/addon-web-links": "^0.11.0",
    "@xterm/addon-webgl": "^0.18.0",
    "@xterm/xterm": "^5.5.0",
    "fuse.js": "^7.1.0",
    "ws": "^8.14.2"
  },
  "type": "module",
  "overrides": {
    "cookie": "^0.7.0",
    "@sveltejs/vite-plugin-svelte": "^5.1.1"
  }
}
