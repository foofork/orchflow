{
  "$schema": "./node_modules/@stryker-mutator/core/schema/stryker-schema.json",
  "packageManager": "npm",
  "reporters": [
    "html",
    "clear-text",
    "progress",
    "json"
  ],
  "testRunner": "vitest",
  "testRunnerNodeArgs": ["--experimental-loader", "./test/loader.mjs"],
  "vitest": {
    "configFile": "vitest.config.ts",
    "dir": "."
  },
  "coverageAnalysis": "perTest",
  "mutate": [
    "src/lib/**/*.ts",
    "src/lib/**/*.js",
    "!src/lib/**/*.test.ts",
    "!src/lib/**/*.spec.ts",
    "!src/lib/**/*.d.ts",
    "!src/test/**/*",
    "!src/**/*.stories.ts"
  ],
  "incremental": true,
  "incrementalFile": ".stryker-tmp/incremental.json",
  "tempDirName": ".stryker-tmp",
  "cleanTempDir": false,
  "concurrency": 4,
  "thresholds": {
    "high": 90,
    "low": 80,
    "break": 75
  },
  "mutator": {
    "plugins": [
      "@stryker-mutator/typescript-checker"
    ]
  },
  "checkers": ["typescript"],
  "tsconfigFile": "tsconfig.json",
  "ignorePatterns": [
    "node_modules",
    "dist",
    "build",
    ".svelte-kit",
    "test-results",
    "docs",
    "scripts"
  ],
  "ignoreStatic": true,
  "timeoutMS": 60000,
  "timeoutFactor": 1.5,
  "maxConcurrentTestRunners": 4,
  "dryRunTimeoutMinutes": 10,
  "mutationTimeoutMS": 300000,
  "plugins": [
    "@stryker-mutator/vitest-runner",
    "@stryker-mutator/typescript-checker",
    "@stryker-mutator/html-reporter"
  ],
  "htmlReporter": {
    "fileName": "test-results/mutation-report.html"
  },
  "jsonReporter": {
    "fileName": "test-results/mutation-report.json"
  },
  "buildCommand": "npm run build",
  "fileLogLevel": "info",
  "logLevel": "info"
}